{
    "@context": "http://www.w3.org/ns/csvw",
    "url": "australian-open-data-portals.csv",
    "dc:title": "Australian Open Data Portals",
    "dc:description": "A list of Australia organisations that publish open data and the portal they publish on",
    "tableSchema": {
        "columns": [{
            "titles": "Organisation"
        },{
            "titles": "URL",
            "datatype": "anyURI"
        },{
            "titles": "Organisation type"
        },{
            "titles": "Portal"
        },{
            "titles": "Region"
        },{
            "titles": "Lat",
            "datatype": {
                "base": "number",
                "minimum": "-90",
                "maximum": "90"
            }
        },{
            "titles": "Lon",
            "datatype": {
                "base": "number",
                "minimum": "-180",
                "maximum": "180"
            }
        }]
    }
}
